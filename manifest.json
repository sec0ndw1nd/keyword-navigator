{
  "manifest_version": 3,
  "name": "Keyword Navigator",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options/options.html",
  "action": {
    "default_popup": "popup/popup.html"
  }
}
