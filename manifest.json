{
  "manifest_version": 3,
  "name": "Keyword Navigator",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "options_page": "options/options.html",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q"
      }
    }
  }
}
